<SCENE>
    <!-- Initial values -->
    <INITIALS>
        <frustum near="0.1" far="500"/>
        <translation x="-10" y="-7" z="0"/>
        <rotation axis="x" angle="25"/>
        <rotation axis="y" angle="90"/>
        <rotation axis="z" angle="-30"/>
        <scale sx="0.25" sy="0.25" sz="0.25"/>
        <reference length="1"/>
    </INITIALS>

    <!-- Illumination and clear colour -->
    <ILLUMINATION>
        <ambient r="0.2" g="0.2" b="0.2" a="1"/>
        <background r="0" g="0" b="0" a="1"/>
    </ILLUMINATION>

    <!-- Lights -->
    <LIGHTS>
        <LIGHT id="light0">
            <enable value="0"/>
            <position x="0" y="10" z="0" w="1"/>
            <ambient r="0.2" g="0.2" b="0.2" a="1.0"/>
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0"/>
            <specular r="1.0" g="1.0" b="1.0" a="1.0"/>
        </LIGHT>

        <LIGHT id="light1">
            <enable value="0"/>
            <position x="30" y="10" z="0" w="1"/>
            <ambient r="0.2" g="0.2" b="0.2" a="1.0"/>
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0"/>
            <specular r="1.0" g="1.0" b="1.0" a="1.0"/>
        </LIGHT>

        <LIGHT id="light2">
            <enable value="0"/>
            <position x="0" y="10" z="20" w="1"/>
            <ambient r="0.2" g="0.2" b="0.2" a="1.0"/>
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0"/>
            <specular r="1.0" g="1.0" b="1.0" a="1.0"/>
        </LIGHT>

        <LIGHT id="light3">
            <enable value="1"/>
            <position x="25" y="17.5" z="20" w="1"/>
            <ambient r="0.2" g="0.2" b="0.2" a="1.0"/>
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0"/>
            <specular r="1.0" g="1.0" b="1.0" a="1.0"/>
        </LIGHT>

        <LIGHT id="light4">
            <enable value="1"/>
            <position x="0" y="0" z="0" w="1"/>
            <ambient r="0.2" g="0.2" b="0.2" a="1.0"/>
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0"/>
            <specular r="1.0" g="1.0" b="1.0" a="1.0"/>
        </LIGHT>

    </LIGHTS>

    <!-- Textures -->
    <TEXTURES>
        <!-- Default texture -->
        <TEXTURE id="defaultTexture">
            <file path=""/>
            <amplif_factor s="1" t="1"/>
        </TEXTURE>

        <!-- Textures for the robot -->
        <TEXTURE id="rust">
            <file path="images_nier_robot/normalrust.jpg"/>
            <amplif_factor s="1" t="1"/>
        </TEXTURE>

        <TEXTURE id="hard_rust">
            <file path="images_nier_robot/hardrust.jpg"/>
            <amplif_factor s="1" t="1"/>
        </TEXTURE>

        <TEXTURE id="light_rust">
            <file path="images_nier_robot/lightrust.jpg"/>
            <amplif_factor s="1" t="1"/>
        </TEXTURE>

        <!-- Textures for robot hat -->
        <TEXTURE id="polkadot">
            <file path="images_nier_robot/polkadot.png"/>
            <amplif_factor s="1" t="1"/>
        </TEXTURE>
        <TEXTURE id="polkadot2">
            <file path="images_nier_robot/polkadot2.jpg"/>
            <amplif_factor s="1.25" t="1"/>
        </TEXTURE>
        <TEXTURE id="fluffy">
            <file path="images_nier_robot/fluffy.jpg"/>
            <amplif_factor s="1" t="1"/>
        </TEXTURE>

        <!-- Disco ball Texture -->
        <TEXTURE id="disco">
            <file path="images_nier_robot/disco.jpg"/>
            <amplif_factor s="1" t="1"/>
        </TEXTURE>

        <!-- Floors and Walls -->
        <TEXTURE id="floor">
            <file path="images_nier_robot/floor.jpg"/>
            <amplif_factor s="4" t="4"/>
        </TEXTURE>

        <TEXTURE id="wall">
            <file path="images_nier_robot/wall.jpg"/>
            <amplif_factor s="4" t="4"/>
        </TEXTURE>

        <TEXTURE id="bar_metal">
            <file path="images_nier_robot/bar_metal.jpg"/>
            <amplif_factor s="1" t="1"/>
        </TEXTURE>

        <TEXTURE id="daftpunk">
            <file path="images_nier_robot/daftpunk.jpg"/>
            <amplif_factor s="20" t="10"/>
        </TEXTURE>

        <TEXTURE id="poster">
            <file path="images_nier_robot/poster.jpg"/>
            <amplif_factor s="8" t="8"/>
        </TEXTURE>

    </TEXTURES>

    <!-- Materials -->
    <MATERIALS>
        <!-- Default material -->
        <MATERIAL id="defaultMaterial">
            <shininess value="1"/>
            <specular r="0.0" g="0.0" b="0.0" a="1"/>
            <diffuse r="0.4" g="0.4" b="0.4" a="1"/>
            <ambient r="0.4" g="0.4" b="0.4" a="1"/>
            <emission r="0.0" g="0.0" b="0.0" a="1"/>
        </MATERIAL>

        <!-- Robot material -->
        <MATERIAL id="latao">
            <shininess value="0.21794872"/>
            <specular r="0.992157" g="0.941176" b="0.807843" a="1"/>
            <diffuse r="0.780392" g="0.568627" b="0.113725" a="1"/>
            <ambient r="0.329412" g="0.223529" b="0.027451" a="1"/>
            <emission r="0.0" g="0.0" b="0.0" a="1"/>
        </MATERIAL>

        <MATERIAL id="prata">
            <shininess value="0.4"/>
            <specular r="0.508273" g="0.508273" b="0.508273" a="1"/>
            <diffuse r="0.50754" g="0.50754" b="0.50754" a="1"/>
            <ambient r="0.19225" g="0.19225" b="0.19225" a="1"/>
            <emission r="0.0" g="0.0" b="0.0" a="1"/>
        </MATERIAL>

        <MATERIAL id="bronze">
            <shininess value="0.2"/>
            <specular r="0.393548" g="0.271906" b="0.166721" a="1"/>
            <diffuse r="0.714" g="0.4284" b="0.18144" a="1"/>
            <ambient r="0.2125" g="0.1275" b="0.054" a="1"/>
            <emission r="0.0" g="0.0" b="0.0" a="1"/>
        </MATERIAL>

        <MATERIAL id="blue_fur">
            <shininess value="0.2"/>
            <specular r="0.020" g="0.044" b="0.118" a="1"/>
            <diffuse r="0.020" g="0.044" b="0.118" a="1"/>
            <ambient r="0.020" g="0.044" b="0.118" a="1"/>
            <emission r="0.0" g="0.0" b="0.0" a="1"/>
        </MATERIAL>

    </MATERIALS>

    <ANIMATIONS>
        <ANIMATION id="walk" speed="1" type="linear">
            <controlpoint xx="20" yy="1.2" zz="18"/>
            <controlpoint xx="40" yy="1.2" zz="25"/>
            <controlpoint xx="20" yy="1.2" zz="15"/>
            <controlpoint xx="40" yy="1.2" zz="25"/>
        </ANIMATION>

    </ANIMATIONS>

    <!-- The nodes -->
    <NODES>
        <ROOT id="root"/>

        <!-- The root node -->
        <NODE id="root">
            <MATERIAL id="defaultMaterial"/>
            <TEXTURE id="null"/>

            <DESCENDANTS>
                <NODEREF id="walls"/>
                <NODEREF id="discoBall"/>
                <NODEREF id="bar"/>
                <NODEREF id="posters"/>
                <NODEREF id="stools"/>
                <NODEREF id="robot"/>
            </DESCENDANTS>
        </NODE>

        <!-- BEGIN walls-->
        <NODE id="walls">
            <MATERIAL id="null"/>
            <TEXTURE id="wall"/>

            <SCALE sx="6" sy="6" sz="6"/>

            <DESCENDANTS>
                <NODEREF id="leftWall"/>
                <NODEREF id="rightWall"/>
                <NODEREF id="frontWall"/>
                <NODEREF id="floorTop"/>
                <NODEREF id="floorBot"/>

            </DESCENDANTS>

        </NODE>

        <NODE id="leftWall">
            <MATERIAL id="null"/>
            <TEXTURE id="null"/>

            <SCALE sx="2" sy="1" sz="1"/>

            <DESCENDANTS>
                <LEAF type="rectangle" args="0 4 4 0"/>
            </DESCENDANTS>

        </NODE>

        <NODE id="rightWall">
            <MATERIAL id="null"/>
            <TEXTURE id="null"/>

            <SCALE sx="2" sy="1" sz="1.7"/>
            <TRANSLATION x="4" y="0" z="4"/>
            <ROTATION axis="y" angle="180"/>

            <DESCENDANTS>
                <LEAF type="rectangle" args="0 4 4 0"/>
            </DESCENDANTS>

        </NODE>

        <NODE id="frontWall">
            <MATERIAL id="null"/>
            <TEXTURE id="null"/>

            <SCALE sx="1" sy="1" sz="1.7"/>
            <TRANSLATION x="8" y="0" z="0"/>
            <ROTATION axis="y" angle="-90"/>

            <DESCENDANTS>
                <LEAF type="rectangle" args="0 4 4 0"/>
            </DESCENDANTS>

        </NODE>

        <NODE id="floorTop">
            <MATERIAL id="null"/>
            <TEXTURE id="floor"/>

            <SCALE sx="2" sy="1" sz="1.7"/>
            <TRANSLATION x="0" y="4" z="0"/>
            <ROTATION axis="x" angle="90"/>

            <DESCENDANTS>
                <LEAF type="rectangle" args="0 4 4 0"/>
            </DESCENDANTS>

        </NODE>

        <NODE id="floorBot">
            <MATERIAL id="null"/>
            <TEXTURE id="floor"/>

            <SCALE sx="2" sy="1" sz="1.7"/>
            <TRANSLATION x="0" y="0" z="4"/>
            <ROTATION axis="x" angle="-90"/>

            <DESCENDANTS>
                <LEAF type="rectangle" args="0 4 4 0"/>
            </DESCENDANTS>

        </NODE>

        <!-- END walls -->

        <!-- BEGIN discoBall-->
        <NODE id="discoBall">
            <MATERIAL id="null"/>
            <TEXTURE id="null"/>

            <TRANSLATION x="25" y="21" z="20"/>

            <DESCENDANTS>
                <NODEREF id="string"/>
                <NODEREF id="ball"/>
            </DESCENDANTS>

        </NODE>

        <NODE id="string">
            <MATERIAL id="null"/>
            <TEXTURE id="null"/>

            <ROTATION axis="x" angle="-90"/>

            <DESCENDANTS>
                <LEAF type="cylinder" args="3 0.1 0.1 2 20 1 1"/>
            </DESCENDANTS>

        </NODE>

        <NODE id="ball">
            <MATERIAL id="null"/>
            <TEXTURE id="disco"/>

            <TRANSLATION x="0" y="-1.6" z="0"/>
            <ROTATION axis="x" angle="-90"/>

            <DESCENDANTS>
                <LEAF type="sphere" args="1.6 20 20"/>
            </DESCENDANTS>

        </NODE>

        <!-- END discoBall-->

        <!-- BEGIN bar-->
        <NODE id="bar">
            <MATERIAL id="null"/>
            <TEXTURE id="bar_metal"/>

            <TRANSLATION x="36" y="1.8" z="5"/>

            <DESCENDANTS>
                <NODEREF id="barLong"/>
                <NODEREF id="barLeft"/>
                <NODEREF id="barRight"/>
            </DESCENDANTS>

        </NODE>

        <NODE id="barLong">
            <MATERIAL id="null"/>
            <TEXTURE id="null"/>

            <SCALE sx="16" sy="3.7" sz="2.5"/>

            <DESCENDANTS>
                <NODEREF id="paralelo"/>
            </DESCENDANTS>

        </NODE>

        <NODE id="barLeft">
            <MATERIAL id="null"/>
            <TEXTURE id="null"/>

            <SCALE sx="1.5" sy="4.7" sz="7"/>
            <TRANSLATION x="-5" y="0.1" z="-0.2"/>

            <DESCENDANTS>
                <NODEREF id="paralelo"/>
            </DESCENDANTS>

        </NODE>

        <NODE id="barRight">
            <MATERIAL id="null"/>
            <TEXTURE id="null"/>

            <SCALE sx="3.8" sy="1" sz="2.9"/>
            <TRANSLATION x="2.6" y="0.27" z="0"/>

            <DESCENDANTS>
                <NODEREF id="paralelo"/>
            </DESCENDANTS>

        </NODE>

        <!-- END bar-->

        <!-- BEGIN posters -->

        <NODE id="posters">
            <MATERIAL id="null"/>
            <TEXTURE id="null"/>

            <DESCENDANTS>
                <NODEREF id="poster1"/>
                <NODEREF id="poster2"/>
            </DESCENDANTS>

        </NODE>

        <NODE id="poster1">
            <MATERIAL id="null"/>
            <TEXTURE id="poster"/>

            <TRANSLATION x="31" y="6.5" z="0.1"/>

            <DESCENDANTS>
                <LEAF type="rectangle" args="0 8 8 0"/>
            </DESCENDANTS>

        </NODE>

        <NODE id="poster2">
            <MATERIAL id="null"/>
            <TEXTURE id="daftpunk"/>

            <ROTATION axis="y" angle="-90"/>
            <TRANSLATION x="13" y="6.8" z="-47"/>

            <DESCENDANTS>
                <LEAF type="rectangle" args="0 10 20 0"/>
            </DESCENDANTS>

        </NODE>

        <!-- END posters-->

        <!-- BEGIN stools-->
        <NODE id="stools">
            <MATERIAL id="null"/>
            <TEXTURE id="clear"/>

            <TRANSLATION x="35" y="2.55" z="9"/>

            <DESCENDANTS>
                <NODEREF id="stool1"/>
                <NODEREF id="stool2"/>
            </DESCENDANTS>

        </NODE>

        <NODE id="stool1">
            <MATERIAL id="null"/>
            <TEXTURE id="null"/>

            <TRANSLATION x="-2.6" y="0" z="0"/>

            <DESCENDANTS>
                <NODEREF id="stool"/>
            </DESCENDANTS>

        </NODE>

        <NODE id="stool2">
            <MATERIAL id="null"/>
            <TEXTURE id="null"/>

            <TRANSLATION x="5" y="0" z="0"/>

            <DESCENDANTS>
                <NODEREF id="stool"/>
            </DESCENDANTS>

        </NODE>

        <NODE id="stool">
            <MATERIAL id="blue_fur"/>
            <TEXTURE id="null"/>

            <ROTATION axis="x" angle="-90"/>

            <DESCENDANTS>
                <LEAF type="cylinder" args="0.7 1.5 1.5 2 20 1 1"/>
                <NODEREF id="leg1"/>
                <NODEREF id="leg2"/>
                <NODEREF id="leg3"/>
                <NODEREF id="leg4"/>
            </DESCENDANTS>

        </NODE>

        <NODE id="leg1">
            <MATERIAL id="null"/>
            <TEXTURE id="null"/>

            <TRANSLATION x="-0.7" y="-0.7" z="-2.6"/>

            <DESCENDANTS>
                <NODEREF id="leg"/>
            </DESCENDANTS>

        </NODE>

        <NODE id="leg2">
            <MATERIAL id="null"/>
            <TEXTURE id="null"/>

            <TRANSLATION x="0.7" y="-0.7" z="-2.6"/>

            <DESCENDANTS>
                <NODEREF id="leg"/>
            </DESCENDANTS>

        </NODE>

        <NODE id="leg3">
            <MATERIAL id="null"/>
            <TEXTURE id="null"/>

            <TRANSLATION x="-0.7" y="0.7" z="-2.6"/>

            <DESCENDANTS>
                <NODEREF id="leg"/>
            </DESCENDANTS>

        </NODE>

        <NODE id="leg4">
            <MATERIAL id="null"/>
            <TEXTURE id="null"/>

            <TRANSLATION x="0.7" y="0.7" z="-2.6"/>

            <DESCENDANTS>
                <NODEREF id="leg"/>
            </DESCENDANTS>

        </NODE>

        <NODE id="leg">
            <MATERIAL id="defaultMaterial"/>
            <TEXTURE id="null"/>

            <DESCENDANTS>
                <LEAF type="cylinder" args="2.7 0.3 0.3 2 20 1 1"/>
            </DESCENDANTS>

        </NODE>

        <!-- END stools-->

        <!-- BEGIN robot -->
        <NODE id="robot">
            <MATERIAL id="prata"/>
            <TEXTURE id="rust"/>

            <SCALE sx="1.4" sy="1.4" sz="1.4"/>
            <TRANSLATION x="17" y="1.2" z="18"/>
            <ROTATION axis="y" angle="180"/>

            <ANIMATIONREFS>
                <ANIMATIONREF id="walk"/>
            </ANIMATIONREFS>

            <DESCENDANTS>
                <NODEREF id="robot_body"/>
                <NODEREF id="robot_head"/>
                <NODEREF id="robot_arms"/>
                <NODEREF id="robot_legs"/>
            </DESCENDANTS>
        </NODE>

        <!-- BEGIN robot_body -->
        <NODE id="robot_body">
            <MATERIAL id="null"/>
            <TEXTURE id="light_rust"/>

            <ROTATION axis="x" angle="-90"/>
            <ROTATION axis="z" angle="-90"/>

            <DESCENDANTS>
                <NODEREF id="robot_body1"/>
                <NODEREF id="robot_body2"/>
                <NODEREF id="robot_body3"/>
            </DESCENDANTS>
        </NODE>

        <NODE id="robot_body1">
            <MATERIAL id="latao"/>
            <TEXTURE id="hard_rust"/>

            <TRANSLATION x="0" y="0" z="3"/>

            <DESCENDANTS>
                <LEAF type="sphere" args="1.6 20 20"/>
            </DESCENDANTS>
        </NODE>

        <NODE id="robot_body2">
            <MATERIAL id="null"/>
            <TEXTURE id="null"/>

            <TRANSLATION x="0" y="0" z="1"/>

            <DESCENDANTS>
                <LEAF type="cylinder" args="2 1.6 1.6 2 20 1 1"/>
            </DESCENDANTS>
        </NODE>

        <NODE id="robot_body3">
            <MATERIAL id="bronze"/>
            <TEXTURE id="null"/>

            <DESCENDANTS>
                <LEAF type="cylinder" args="1 1.6 1.6 2 20 0 1"/>
            </DESCENDANTS>
        </NODE>
        <!-- END robot_body -->

        <!-- BEGIN robot_head -->
        <NODE id="robot_head">
            <MATERIAL id="prata"/>
            <TEXTURE id="null"/>

            <TRANSLATION x="0.7" y="3.75" z="0.1"/>

            <DESCENDANTS>
                <NODEREF id="robot_hat"/>
                <NODEREF id="robot_head1"/>
                <NODEREF id="robot_eyes"/>
            </DESCENDANTS>
        </NODE>

        <NODE id="robot_hat">
            <MATERIAL id="prata"/>
            <TEXTURE id="null"/>

            <TRANSLATION x="0" y="0.88" z="0"/>

            <DESCENDANTS>
                <NODEREF id="robot_hat_left"/>
                <NODEREF id="robot_hat_right"/>
            </DESCENDANTS>
        </NODE>

        <NODE id="robot_hat_left">
            <MATERIAL id="null"/>
            <TEXTURE id="polkadot"/>

            <SCALE sx="1.25" sy="1.25" sz="1.25"/>
            <TRANSLATION x="0" y="0" z="-0.3"/>
            <ROTATION axis="y" angle="20"/>

            <DESCENDANTS>
                <NODEREF id="robot_hat_base"/>
                <NODEREF id="robot_hat_body"/>
                <NODEREF id="robot_hat_pompom"/>
            </DESCENDANTS>
        </NODE>

        <NODE id="robot_hat_right">
            <MATERIAL id="null"/>
            <TEXTURE id="polkadot2"/>

            <SCALE sx="1.25" sy="1.25" sz="1.25"/>
            <TRANSLATION x="0" y="0" z="0.3"/>
            <ROTATION axis="y" angle="180"/>
            <ROTATION axis="y" angle="-20"/>

            <DESCENDANTS>
                <NODEREF id="robot_hat_base"/>
                <NODEREF id="robot_hat_body"/>
                <NODEREF id="robot_hat_pompom"/>
            </DESCENDANTS>
        </NODE>

        <NODE id="robot_hat_base">
            <MATERIAL id="null"/>
            <TEXTURE id="fluffy"/>

            <TRANSLATION x="0" y="-0.25" z="0"/>
            <ROTATION axis="x" angle="-100"/>

            <DESCENDANTS>
                <LEAF type="cylinder" args="0.5 1 0.9 2 20 1 1"/>
            </DESCENDANTS>
        </NODE>

        <NODE id="robot_hat_body">
            <MATERIAL id="null"/>
            <TEXTURE id="null"/>

            <TRANSLATION x="0" y="0" z="0"/>
            <SCALE sx="1.1" sy="1.1" sz="1.1"/>
            <ROTATION axis="x" angle="-30"/>

            <DESCENDANTS>
                <LEAF type="patch" args="20 20">
                    <CPLINE>
                        <CPOINT xx="0.75" yy="0" zz="0" ww="1"/>
                        <CPOINT xx="0.75" yy="0" zz="1.0" ww="1"/>
                        <CPOINT xx="-0.75" yy="0" zz="1.0" ww="1"/>
                        <CPOINT xx="-0.75" yy="0" zz="0" ww="1"/>
                    </CPLINE>
                    <CPLINE>
                        <CPOINT xx="0.5" yy="1" zz="0" ww="1"/>
                        <CPOINT xx="0.5" yy="1" zz="1" ww="1"/>
                        <CPOINT xx="-0.5" yy="1" zz="1" ww="1"/>
                        <CPOINT xx="-0.5" yy="1" zz="0" ww="1"/>
                    </CPLINE>
                    <CPLINE>
                        <CPOINT xx="0.25" yy="2" zz="-0.5" ww="1"/>
                        <CPOINT xx="0.25" yy="2" zz="-0.5" ww="1"/>
                        <CPOINT xx="-0.25" yy="2" zz="-0.5" ww="1"/>
                        <CPOINT xx="-0.25" yy="2" zz="-0.5" ww="1"/>
                    </CPLINE>
                    <CPLINE>
                        <CPOINT xx="0.25" yy="1.5" zz="-1" ww="1"/>
                        <CPOINT xx="0.25" yy="1.75" zz="-1" ww="1"/>
                        <CPOINT xx="-0.25" yy="1.75" zz="-1" ww="1"/>
                        <CPOINT xx="-0.25" yy="1.5" zz="-1" ww="1"/>
                    </CPLINE>
                    <CPLINE>
                        <CPOINT xx="0.1" yy="1" zz="-1.25" ww="1"/>
                        <CPOINT xx="0.1" yy="1.25" zz="-1.25" ww="1"/>
                        <CPOINT xx="-0.1" yy="1.25" zz="-1.25" ww="1"/>
                        <CPOINT xx="-0.1" yy="1" zz="-1.25" ww="1"/>
                    </CPLINE>
                </LEAF>
                <LEAF type="patch" args="20 20">
                    <CPLINE>
                        <CPOINT xx="-0.75" yy="0" zz="0" ww="1"/>
                        <CPOINT xx="-0.75" yy="0" zz="-1.0" ww="1"/>
                        <CPOINT xx="0.75" yy="0" zz="-1.0" ww="1"/>
                        <CPOINT xx="0.75" yy="0" zz="0" ww="1"/>
                    </CPLINE>
                    <CPLINE>
                        <CPOINT xx="-0.5" yy="1" zz="0" ww="1"/>
                        <CPOINT xx="-0.5" yy="1" zz="-1" ww="1"/>
                        <CPOINT xx="0.5" yy="1" zz="-1" ww="1"/>
                        <CPOINT xx="0.5" yy="1" zz="0" ww="1"/>
                    </CPLINE>
                    <CPLINE>
                        <CPOINT xx="-0.25" yy="2" zz="-0.5" ww="1"/>
                        <CPOINT xx="-0.25" yy="2" zz="-0.5" ww="1"/>
                        <CPOINT xx="0.25" yy="2" zz="-0.5" ww="1"/>
                        <CPOINT xx="0.25" yy="2" zz="-0.5" ww="1"/>
                    </CPLINE>
                    <CPLINE>
                        <CPOINT xx="-0.25" yy="1.5" zz="-1" ww="1"/>
                        <CPOINT xx="-0.25" yy="1.25" zz="-1" ww="1"/>
                        <CPOINT xx="0.25" yy="1.25" zz="-1" ww="1"/>
                        <CPOINT xx="0.25" yy="1.5" zz="-1" ww="1"/>
