            <MATERIAL id="defaultMaterial"/>
            <TEXTURE id="null"/>

            <DESCENDANTS>
                <NODEREF id="shiftago"/>
            </DESCENDANTS>
        </NODE>

        <NODE id="shiftago" dimension="7">
            <MATERIAL id="null"/>
            <TEXTURE id="null"/>

            <DESCENDANTS>
